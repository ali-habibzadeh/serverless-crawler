{
  "name": "serverless-crawler",
  "version": "0.1.3",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "tsc",
    "copyall": "cp package.json dist/package.json && cp -a binary-assets/. dist/",
    "postbuild": "yarn copyall && cd dist && npm install --only=production && node-prune",
    "watch": "tsc -w",
    "integration": "jest --config=jest.integration.config.js",
    "deploy": "yarn build && cdk deploy --require-approval never --outputs-file outputs.json",
    "destroy": "cdk destroy -f",
    "synth": "cdk synth"
  },
  "devDependencies": {
    "@aws-cdk/assert": "1.51.0",
    "@aws-cdk/aws-apigateway": "^1.51.0",
    "@aws-cdk/aws-apigatewayv2": "^1.51.0",
    "@aws-cdk/aws-codebuild": "^1.51.0",
    "@aws-cdk/aws-codecommit": "^1.51.0",
    "@aws-cdk/aws-codepipeline": "^1.51.0",
    "@aws-cdk/aws-codepipeline-actions": "^1.51.0",
    "@aws-cdk/aws-dax": "^1.51.0",
    "@aws-cdk/aws-dynamodb": "^1.51.0",
    "@aws-cdk/aws-ecr-assets": "^1.51.0",
    "@aws-cdk/aws-ecs": "^1.51.0",
    "@aws-cdk/aws-ecs-patterns": "^1.51.0",
    "@aws-cdk/aws-elasticloadbalancingv2": "^1.51.0",
    "@aws-cdk/aws-events": "^1.51.0",
    "@aws-cdk/aws-events-targets": "^1.51.0",
    "@aws-cdk/aws-iam": "^1.51.0",
    "@aws-cdk/aws-kinesis": "^1.51.0",
    "@aws-cdk/aws-kinesisfirehose": "^1.51.0",
    "@aws-cdk/aws-lambda": "^1.51.0",
    "@aws-cdk/aws-lambda-destinations": "^1.51.0",
    "@aws-cdk/aws-lambda-event-sources": "^1.51.0",
    "@aws-cdk/aws-s3": "^1.51.0",
    "@aws-cdk/aws-servicediscovery": "^1.51.0",
    "@aws-cdk/aws-sqs": "^1.51.0",
    "@deepcrawl/tslint-config": "git://github.com/deepcrawl/tslint-config-deepcrawl.git",
    "@types/async": "^3.2.3",
    "@types/aws-lambda": "^8.10.59",
    "@types/convict": "^5.2.1",
    "@types/jest": "^26.0.4",
    "@types/lodash": "^4.14.157",
    "@types/minimatch": "^3.0.3",
    "@types/moment": "^2.13.0",
    "@types/node": "14.0.23",
    "@types/parquetjs": "^0.10.2",
    "@types/puppeteer-core": "^2.0.0",
    "@types/source-map-support": "^0.5.2",
    "@types/xml2js": "^0.4.5",
    "aws-cdk": "1.51.0",
    "aws-lambda": "^1.0.5",
    "devtools-protocol": "^0.0.787341",
    "jest": "^26.1.0",
    "node-prune": "^1.0.2",
    "ts-jest": "^26.1.2",
    "ts-node": "^8.10.2",
    "tslint": "^6.1.1",
    "typescript": "^3.9.6"
  },
  "dependencies": {
    "@aws-cdk/aws-glue": "^1.51.0",
